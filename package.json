{
  "name": "css-var-color-decorator",
  "displayName": "css-var-color-decorator",
  "description": "provide color decorator for css variables",
  "version": "1.3.0",
  "author": {
    "name": "meouwu-dev",
    "url": "https://github.com/meouwu-dev"
  },
  "publisher": "meouwu",
  "repository": {
    "type": "git",
    "url": "https://github.com/meouwu-dev/css-var-color-decorator"
  },
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "color.hex2Hsl",
        "title": "color.hex2Hsl"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "CSS Var Color Decorator",
      "properties": {
        "cssVarColorDecorator.supportedLanguages": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": ["css", "scss", "tailwindcss"],
          "description": ""
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.3",
    "@types/node": "18.x",
    "@types/tinycolor2": "^1.4.6",
    "@types/vscode": "^1.84.0",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@vscode/test-electron": "^2.3.6",
    "eslint": "^8.52.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "tinycolor2": "^1.6.0"
  }
}
